@Article{Becker1998177,
  Title                    = {Chemistry and kinetics of chemical vapor deposition of pyrocarbon: II pyrocarbon deposition from ethylene, acetylene and 1,3-butadiene in the low temperature regime},
  Author                   = {A. Becker and K.J. Hüttinger},
  Journal                  = {Carbon},
