@Article{Sproge1988,
  Title                    = {Experimental and theoretical studies of gas consumption in the gas carburizing process},
  Author                   = {Sproge, Lars and {\AA}gren, John},
  Journal                  = {Journal of Heat Treating},
  Year                     = {1988},
  Number                   = {1},
  Pages                    = {9-19},
  Volume                   = {6},

  Abstract                 = {Abstract. In this work, the gas carburizing process is studied theoretically and experimentally. In particular, the possibility of lowering the gas consumption is investigated. A mathematical model which accounts for the carbon diffusion in the steel, the surface reactions on the steel, and the change in the furnace atmosphere composition caused by the carburizing reactions is developed. Two experimental series are performed in order to investigate the dependence of the final carbon profile, the atmosphere composition, etc. on the gas exchange rate. Both experiments and calculations reveal that it is possible to decrease the gas exchange rate considerably and thus the gas consumption compared to what is usually applied. At very low exchange rates an increased CH4 content is observed. Possible drawbacks of this increase are discussed, such as the increased risk for sooting, etc.},
  Comment                  = {cited in 2016MATSCIA1},
  Doi                      = {10.1007/BF02833160},
  File                     = {Article:references/1988_Sproge_Carburizing_Kinetics.pdf:PDF},
  ISSN                     = {0190-9177},
  Keywords                 = {process, carburizing, atmospheres},
  Language                 = {English},
  Publisher                = {Springer-Verlag},
  Review                   = {SUMMARY The gas carburizing process has been studied experimentally and theoretically. The theoretical studies are based on computer simulations of the carbon diffusion, the surface reactions, and the reactions in the furnace atmosphere. Special attention has been paid to the effect of lowering the gas exchange rate. The experimental results show that the result of the carburizing does not depend very much on the gas exchange rate. Both the experimental results and the theoretical calculations show that the most important effect o f lowering the gas exchange rate is that the atmosphere is displaced from equilibrium. This is manifested in a much higher methane content than at equilibrium. It is suggested that a high methane content may be partly responsible for problems reported in connection with nonequilibrium atmospheres such as sooting.}
}

