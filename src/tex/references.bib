@Article{Norstrom1976,
  Title                    = {On the yield strength of quenched low-carbon martensite},
  Author                   = {L.-A. Norstrom},
  Journal                  = {Scandinavian Journal of Metallurgy},
  Year                     = {1976},
  Pages                    = {159-165},
  Volume                   = {5},

  Abstract                 = {The tensile yield of quenched lath martensite has been investigated and analysed with respect to grain size, substructure and carbon content ( less than 0. 1\%C) in 4-6\% Mn steels and in 5 and 9\% Ni steels. The following main contributions to yield strength have been quantitatively distinguished: substitutional element solution hardening, packet boundaries (high-angle), lath boundaries (low-angle) and dislocations within laths. The contribution from martensite packets follows an ordinary Petch-Hall relation. In the Mn steels the Petch-Hall coefficient is found to be about the same as in ferritic carbon steels, but is strikingly lower in the 5 and 9% Ni steel. A fairly constant contribution of about 150 N/mm**2 is established for the martensite lath boundaries. The dislocation density within the laths is high and is consequently responsible for a significant part of the yield strength. The dislocation density is found to increase markedly with carbon content, which seems to be the apparent reason for the steep increase in yield strength with carbon content. When all these contributions are taken into account there remains no part of the yield strength which can be associated with any strong solid solution hardening from carbon. Thus quenched low-carbon lath martensite might be compared to a heavily cold-worked ferrite and an increase in carbon content in the martensite then is analogous to an increased degree of deformation.},
  Comment                  = {cited in 2016MATSCIA1},
  File                     = {Missing:references/1976_Norstrom_Martensite_Hardness.txt:Text},
  Keywords                 = {metallurgy, martensite},
  Review                   = {Section 4 is the main for my thesis.}
}

