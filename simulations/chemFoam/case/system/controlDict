FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application       chemFoam;

startFrom         startTime;

startTime         0;

stopAt            endTime;

endTime           1.00;

deltaT            0.10;

maxDeltaT         0.01;

adjustTimeStep    on;

writeControl      adjustableRunTime;

writeInterval     1.0;

purgeWrite        0;

writeFormat       ascii;

writeCompression  off;

timeFormat        fixed;

timePrecision     6;

runTimeModifiable yes;

DebugSwitches
{
    SolverPerformance   0;
}


